statechart sc {
    events {
		e1;
		e2;
    }

	x: int;
	entry:{x:=10;}
	exit:{}
	state A {
		xA: boolean;
		entry:{x:=10;}
		exit:{}
	}
	state B {
		xB: int;
	}

    transition t1 {
		source: sc.A;
		destination: sc.B;
		trigger: e1;
		guard: true;
		action: x := x+1;
	}

    transition t2 {
		source: sc.A;
		destination: sc.B;
		trigger: e1;
		guard: true;
		action:{}
	}
}
